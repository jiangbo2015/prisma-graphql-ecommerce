{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix": "eslint ./src/components/*.tsx --fix",
    "schema": "apollo client:codegen --target typescript",
    "download": "apollo service:download",
    "generate": "yarn download && apollo codegen:generate --localSchemaFile=schema.json ./src/__generated__/ --target=typescript --tagName=gql --outputFlat  --watch --includes=./src/graphql/*"
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "graphql": "^15.5.3",
    "prop-types": "^15.7.2",
    "ra-data-json-server": "^3.18.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-material-ui-form-validator": "^2.1.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "server": "^1.0.35",
    "type-graphql": "^1.1.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-material-ui-form-validator": "^2.1.1",
    "@types/react-router-dom": "^5.1.8",
    "typescript": "^4.4.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
